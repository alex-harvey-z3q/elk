{
  "aliases": {},
  "index_patterns": [
    "logstash-*"
  ],
  "mappings": {
    "_default_": {
      "dynamic_templates": [
        {
          "string_fields": {
            "mapping": {
              "fields": {
                "raw": {
                  "ignore_above": "256",
                  "index": "not_analyzed",
                  "type": "text"
                }
              },
              "index": "analyzed",
              "omit_norms": "true",
              "type": "text"
            },
            "match": "*",
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@version": {
          "index": "false",
          "type": "text"
        },
        "geoip": {
          "dynamic": "true",
          "properties": {
            "location": {
              "type": "geo_point"
            }
          },
          "type": "object"
        }
      }
    }
  },
  "order": "0",
  "settings": {
    "index": {
      "refresh_interval": "5s"
    }
  }
}
